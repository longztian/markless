{
  "name": "markx",
  "version": "0.0.1",
  "description": "yet another markup language, inspired by markdown",
  "main": "dist/markx.js",
  "scripts": {
    "lint": "node_modules/.bin/eslint src bin",
    "build": "rm -rf build && mkdir -p build/test && node bin/build.js",
    "test": "mocha build/test",
    "dist": "mkdir -p dist && cp build/markx.js dist && node_modules/.bin/uglifyjs dist/markx.js -m -c -p relative --source-map dist/markx.min.js.map -o dist/markx.min.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/longztian/markx.git"
  },
  "keywords": [
    "markx",
    "markdown",
    "markup"
  ],
  "author": "Longzhang Tian",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/longztian/markx/issues"
  },
  "homepage": "https://github.com/longztian/markx#readme",
  "devDependencies": {
    "babel-cli": "^6.11.4",
    "babel-core": "^6.14.0",
    "babel-plugin-external-helpers": "^6.8.0",
    "babel-preset-es2015": "^6.14.0",
    "chai": "^3.5.0",
    "eslint": "^3.3.1",
    "eslint-config-airbnb": "^10.0.1",
    "eslint-plugin-import": "^1.14.0",
    "eslint-plugin-jsx-a11y": "^2.1.0",
    "eslint-plugin-react": "^6.1.2",
    "mocha": "^3.0.2",
    "readdirp": "^2.1.0",
    "rollup": "^0.34.10",
    "rollup-plugin-babel": "^2.6.1",
    "uglify-js": "^2.7.3"
  }
}
